<div class="uk-form-horizontal uk-margin-large">
{{ form_start(form) }}
    <input type="hidden" name="_token" id="tokenid" value="{{ csrf_token('delete') }}"/>
    <div class="uk-flex">
        <div class="uk-width-1-3"></div>
        <div  class="uk-width-1-3">
        <div class="uk-margin">
            <label class="uk-form-label" for="form-horizontal-text">{{ field_label(form.nom) }}</label>
            <div class="uk-form-controls">
                <input class="uk-input" id="form-horizontal-text" type="text" name="{{ field_name(form.nom) }}" value="{{ field_value(form.nom) }}">
            </div>
        </div>

        <div class="uk-margin">
            <label class="uk-form-label" for="form-horizontal-text">{{ field_label(form.dateHeureDebut) }}</label>
            <div class="uk-form-controls">
                {{ form_widget(form.dateHeureDebut) }}
            </div>
        </div>

        <div class="uk-margin">
            <label class="uk-form-label" for="form-horizontal-text">{{ field_label(form.duree) }}</label>
            <div class="uk-form-controls">
                <input class="uk-input" id="form-horizontal-text" type="number" min="1" name="{{ field_name(form.duree) }}" value="{{ field_value(form.duree) }}">
            </div>
        </div>

        <div class="uk-margin">
            <label class="uk-form-label" for="form-horizontal-text">{{ field_label(form.nbInscriptionsMax) }}</label>
            <div class="uk-form-controls">
                <input class="uk-input" id="form-horizontal-text" type="number" min="1" name="{{ field_name(form.nbInscriptionsMax) }}" value="{{ field_value(form.nbInscriptionsMax) }}">
            </div>
        </div>

        <div class="uk-margin">
            <label class="uk-form-label" for="form-horizontal-text">{{ field_label(form.dateLimiteInscription) }}</label>
            <div class="uk-form-controls">
                {{ form_widget(form.dateLimiteInscription) }}
            </div>
        </div>

        <div class="uk-margin ">
            <label class="uk-form-label" for="form-horizontal-text">{{ field_label(form.infosSortie) }}</label>
            <div class="uk-form-controls">
                <input class="uk-input" id="form-horizontal-text" type="text" name="{{ field_name(form.infosSortie) }}" value="{{ field_value(form.infosSortie) }}">
            </div>
        </div>

        <div class="uk-margin">
            <label class="uk-form-label" for="form-horizontal-text">{{ form_label(form.campus) }}</label>
            <div class="uk-form-controls">
                {{ form_widget(form.campus) }}
            </div>
        </div>

        <div class="uk-margin ">
            <label class="uk-form-label" for="form-horizontal-text">{{ form_label(form.lieu) }}</label>
            <div class="uk-form-controls">
                {{ form_widget(form.lieu) }}
            </div>
        </div>
        </div>
        <div  class="uk-width-1-3"></div>
    </div>

        <div class="uk-flex">
            <div class="uk-width-1-3"></div>
            <div class="uk-width-1-3">
                <button class="uk-button uk-button-default uk-width-1-1">{{ button_label|default('Publier la sortie') }}</button>
                {{ form_end(form,{render_rest: false}) }}
            </div>
            <div class="uk-width-1-3"></div>
        </div>
</div>
