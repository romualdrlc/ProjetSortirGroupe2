    </div>
    <div class="uk-margin">
        <div class="uk-form-controls">
            {{ form_row(form.nom) }}
        </div>
    </div>
    <div class="uk-margin">
        <div class="uk-form-controls">
            {{ form_row(form.codePostal) }}
