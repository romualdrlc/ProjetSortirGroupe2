            <button class="uk-button uk-button-default">{{ button_label|default('Sauvegarder') }}</button>
            <a class="uk-button uk-button-default" href="{{ path('app_ville_index') }}">Retour Ã  la liste</a>
        </div>
        <div class="uk-width-1-3"></div>
